#include <stdio.h>
void printPattern(int n);
int main() {
    printf("Name : M.Vignesh\n");
    printf("Reg no : 192211849\n");
    int n = 4; 
    printPattern(n);
    return 0;
}
void printPattern(int n) {
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n - i; j++) {
            printf("   ");
        }
        for (int k = 1; k <= i; k++) {
            printf("%d ", k);
        }
        printf("\n");
    }
}
