#include <stdio.h>
int binomialCoefficient(int n, int k);
int main() {
    printf("Name : M.Vignesh\n");
    printf("Reg no : 192211849\n");
    int n = 5;  
    int k = 2;  
    int result = binomialCoefficient(n, k);
    printf("Binomial coefficient C(%d, %d) is: %d\n", n, k, result);
    return 0;
}
int binomialCoefficient(int n, int k) {
    int C[n + 1][k + 1];
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= k; j++) {
            if (j == 0 || j == i) {
                C[i][j] = 1;
            } else {
                C[i][j] = C[i - 1][j - 1] + C[i - 1][j];
            }
        }
    }
    return C[n][k];
}
